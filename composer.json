{
    "name": "luxid/sentinel",
    "description": "Session-based authentication package for Luxid PHP Framework",
    "type": "library",
    "license": "MIT",
    "keywords": ["luxid", "authentication", "auth", "session", "guard", "sentinel"],
    "authors": [
        {
            "name": "Jhay",
            "email": "jhay@luxid.dev"
        }
    ],
    "require": {
        "php": "^8.0",
        "luxid/engine": "^0.4.2"
    },
    "autoload": {
        "psr-4": {
            "Luxid\\Sentinel\\": "src/"
        },
        "files": [
            "src/Support/helpers.php"
        ]
    },
    "extra": {
        "luxid": {
            "providers": [
                "Luxid\\Sentinel\\Providers\\SentinelServiceProvider"
            ],
            "commands": {
                "sentinel:install": "Luxid\\Sentinel\\Console\\InstallCommand"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
